{
	"name": "movie-tracker",
	"version": "1.0.0",
	"description": "1. With node installed, open /movie-tracker/ and type **node server.js** 2. Open http://localhost:8081 in a browser window.",
	"main": "server.js",
	"scripts": {
		"start": "node server.js",
		"dev": "nodemon server.js",
		"seed": "node scripts/init_database.js",
		"install:ui": "npm --prefix ui install --legacy-peer-deps",
		"build:ui": "npm --prefix ui run build",
		"build:all": "npm run install:ui && npm run build:ui"
	},
	"engines": {
		"node": "20.x"
	},
	"keywords": [],
	"author": "Richard Herlihy",
	"license": "ISC",
	"dependencies": {
		"bcrypt": "^5.1.1",
		"connect-flash": "^0.1.1",
		"cors": "^2.8.5",
		"csvtojson": "^2.0.10",
		"express": "^4.16.3",
		"express-fileupload": "^1.2.0",
		"express-session": "^1.17.1",
		"mongodb": "^3.6.3",
		"multer": "1.4.5-lts.1",
		"passport": "^0.4.1",
		"passport-local": "^1.0.0",
		"react-icons": "^4.1.0",
		"sweetalert-react": "^0.4.11",
		"uuidv4": "^6.2.6"
	},
	"devDependencies": {
		"nodemon": "^3.1.11"
	}
}
